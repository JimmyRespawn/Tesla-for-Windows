<Application
    x:Class="TeslaMurphy.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters="using:TeslaMurphy.Converters"
    xmlns:local="using:TeslaMurphy">
    <Application.Resources>
        <ResourceDictionary>
            <converters:LockStateToIconConverter x:Key="LockStateToIconConverter" />
            <converters:BatteryStateToIconConverter x:Key="BatteryStateToIconConverter" />
            <converters:BooleanToOpenCloseConverter x:Key="BooleanToOpenCloseConverter" />
            <converters:CarModelToImageConverter x:Key="CarModelToImageConverter" />
            <converters:CarModelToNameConverter x:Key="CarModelToNameConverter" />
            <converters:ChargeStateVisibilityConverter x:Key="ChargeStateVisibilityConverter" />
            <converters:UpdateTimestampToDesConverter x:Key="UpdateTimestampToDesConverter" />
            <ResourceDictionary.MergedDictionaries>
                <XamlControlsResources xmlns="using:Microsoft.UI.Xaml.Controls" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
